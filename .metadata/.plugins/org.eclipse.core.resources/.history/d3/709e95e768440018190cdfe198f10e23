import java.util.List;

public class Application {
	
	
	public static void print(List<Customer> custList){
		for(Customer eaCustomer : custList){
			System.out.println(eaCustomer);
		}
		
	}

	public static void main(String[] args) {


		CustomerManager mgr = new CustomerManager();
		mgr.add(new Customer(101, "Ramesh", 9999999));
		mgr.add(new Customer(102, "Suresh", 8888888));
		mgr.add(new Customer(103, "Madesh", 7777777));
		mgr.add(new Customer(104, "Kamlesh",666666));
		
		
	}

}
